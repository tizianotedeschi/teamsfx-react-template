{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.17/MicrosoftTeams.schema.json",
  "manifestVersion": "1.17",
  "version": "1.0.0",
  "id": "${{TEAMS_APP_ID}}",
  "developer": {
    "name": "Teams App, Inc.",
    "websiteUrl": "${{TAB_ENDPOINT}}",
    "privacyUrl": "${{TAB_ENDPOINT}}/index.html#/privacy",
    "termsOfUseUrl": "${{TAB_ENDPOINT}}/index.html#/termsofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "teamsfx-react-template-${{APP_NAME_SUFFIX}}",
    "full": "Full name for teamsfx-react-template"
  },
  "description": {
    "short": "Short description of teamsfx-react-template",
    "full": "Full description of teamsfx-react-template"
  },
  "accentColor": "#FFFFFF",
  "bots": [],
  "composeExtensions": [],
  "staticTabs": [
    {
      "entityId": "index",
      "name": "Personal Tab",
      "contentUrl": "${{TAB_ENDPOINT}}/index.html#/tab",
      "websiteUrl": "${{TAB_ENDPOINT}}/index.html#/tab",
      "scopes": ["personal"]
    }
  ],
  "permissions": ["identity"],
  "validDomains": ["${{TAB_HOSTNAME}}"],
  "webApplicationInfo": {
    "id": "${{AAD_APP_CLIENT_ID}}",
    "resource": "api://${{TAB_DOMAIN}}/${{AAD_APP_CLIENT_ID}}"
  }
}
